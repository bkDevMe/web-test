{
  "configurations": [
    {
      "name": "Attach to Chrome",
      "port": 9222,
      "request": "attach",
      "type": "chrome",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "runtimeArgs": ["--auto-open-devtools-for-tabs"],
      "url": "http://localhost:2333/main.html",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
